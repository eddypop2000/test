      kubectl rollout status deployment ${prefixPod}-${shortEnv} -n ${namespace}
      &quot;&quot;&quot;
    }
  }
}

def build() {
  echo &quot;Process build for ${env.BRANCH_NAME} branch&quot;
  buildDocker()
  deployToK8s()
  currentBuild.result = &apos;SUCCESS&apos;

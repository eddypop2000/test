def buildDocker() {
  stage(&apos;Build docker&apos;) {
    container(&apos;awscli&apos;) {
      DOCKER_LOGIN = sh (
        script: &apos;aws ecr get-login --no-include-email --region ap-southeast-1&apos;,
        returnStdout: true
      ).trim()
    }
    container(&apos;docker&apos;) {
      REGISTRY_URL = &quot;${privateEcr}/${prefixPod}:${shortEnv}&quot;
      sh &quot;&quot;&quot;
      ls -la
      docker build -f Dockerfile.${shortEnv} --network=host -t ${REGISTRY_URL} --pull=true .
      ${DOCKER_LOGIN}
      docker push ${REGISTRY_URL}
      &quot;&quot;&quot;
    }
  }
}

def deployToK8s() {
  stage(&apos;Deploy to k8s&apos;) {
    container(&apos;kubectl&apos;) {
      sh &quot;&quot;&quot;
